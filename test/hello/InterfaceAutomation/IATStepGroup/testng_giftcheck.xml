<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="apitest">
	<test name="GiftCheck_BindEmail">
		<parameter name="giftDataGeneration" value="BusinessDataModule"></parameter>
		<parameter name="giftParameterSetKey" value="userLimit"></parameter>
		<parameter name="giftParameterSetValue" value='[{"code":"userBindEmail"}]'></parameter>
		<parameter name="giftCheckBindEmail" value="UserLimit_UserBindEmail"></parameter>
		<classes>
			<class name="com.baidu.gameqa.generation.GiftCheckExecutor">
			<methods>
				<include name="giftCheckBindEmail"></include>
			</methods>
			</class>
		</classes>
	</test>
	
	<test name="GiftCheck_NoBindEmail">
		<parameter name="giftCheckNoBindEmail" value="UserLimit_UserNoBindEmail"></parameter>
		<classes>
			<class name="com.baidu.gameqa.generation.GiftCheckExecutor">
			<methods>
				<include name="giftCheckNoBindEmail"></include>
			</methods>
			</class>
		</classes>
	</test>
	
